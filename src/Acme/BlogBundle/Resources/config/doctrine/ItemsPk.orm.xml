<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ItemsPk" table="items_pk">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="prod" type="string" column="prod" length="7" nullable="true"/>
    <field name="quant" type="smallint" column="quant" nullable="true"/>
    <field name="val" type="decimal" column="val" nullable="true"/>
    <field name="invnum" type="integer" column="invnum" nullable="false"/>
    <field name="comment" type="string" column="comment" length="80" nullable="true"/>
  </entity>
</doctrine-mapping>
