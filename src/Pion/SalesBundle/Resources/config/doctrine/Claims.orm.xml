<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Pion\SalesBundle\Entity\Claims" table="claims">
    <field name="claimcount" type="smallint" column="claimcount" nullable="false"/>
    <field name="invnum" type="integer" column="invnum" nullable="false"/>
    <field name="journal" type="string" column="journal" length="30" nullable="false"/>
    <field name="volume" type="string" column="volume" length="10" nullable="false"/>
    <field name="issue" type="string" column="issue" length="10" nullable="false"/>
    <field name="claimdate" type="date" column="claimdate" nullable="true"/>
    <field name="response" type="string" column="response" length="40" nullable="true"/>
    <field name="comment" type="string" column="comment" length="80" nullable="true"/>
  </entity>
</doctrine-mapping>
