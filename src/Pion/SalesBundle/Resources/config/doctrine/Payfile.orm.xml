<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Pion\SalesBundle\Entity\Payfile" table="payfile">
    <field name="refno" type="integer" column="refno" nullable="false"/>
    <field name="pdate" type="date" column="pdate" nullable="true"/>
    <field name="pname" type="string" column="pname" length="30" nullable="true"/>
    <field name="chqno" type="string" column="chqno" length="20" nullable="true"/>
    <field name="paycurr" type="smallint" column="paycurr" nullable="true"/>
    <field name="payamt" type="decimal" column="payamt" nullable="true"/>
    <field name="invcurr" type="smallint" column="invcurr" nullable="true"/>
    <field name="invamt" type="decimal" column="invamt" nullable="true"/>
    <field name="prodd" type="string" column="prodd" length="10" nullable="true"/>
    <field name="usable" type="string" column="usable" nullable="true"/>
    <field name="journal" type="string" column="journal" nullable="true"/>
    <field name="invno" type="string" column="invno" length="40" nullable="true"/>
    <field name="pcomm" type="string" column="pcomm" length="80" nullable="true"/>
    <field name="account" type="string" column="account" nullable="true"/>
    <field name="pcntry" type="string" column="pcntry" nullable="true"/>
  </entity>
</doctrine-mapping>
